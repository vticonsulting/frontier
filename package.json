{
  "name": "@vticonsulting/frontier",
  "version": "0.1.0",
  "private": "true",
  "scripts": {
    "dev": "vite --port 8700",
    "build": "babel src/main.js --extensions .js,.ts,.tsx --out-dir dist",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "check-types": "tsc",
    "validate": "npm-run-all --parallel check-types check-format lint build",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "setup": "npm run validate"
  },
  "dependencies": {
    "vue": "^3.0.0-rc.13",
    "vue-router": "^4.0.0-beta.13",
    "vuex": "^4.0.0-beta.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@headlessui/vue": "^0.1.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "@vue/compiler-sfc": "^3.0.0-rc.1",
    "@vue/test-utils": "^2.0.0-beta.6",
    "axios": "^0.20.0",
    "date-fns": "^2.16.1",
    "marked": "^1.2.0",
    "eslint": "^7.10.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-vue": "^7.0.0-0",
    "faker": "^5.1.0",
    "flush-promises": "^1.0.2",
    "jest-serializer-vue-tjw": "^3.14.0",
    "json-server": "^0.16.2",
    "lodash": "^4.17.20",
    "tailwindcss": "^1.8.10",
    "husky": "^4.3.0",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3",
    "vite": "^1.0.0-rc.4",
    "vue-jest": "^5.0.0-alpha.4"
  }
}
